class Solution(object):
    def maxDepth(self, s):
        """
        :type s: str
        :rtype: int
        """

        count = 0
        max = 0

        for i in s:
            if i=='(':
                count+=1

                if count>max:
                    max=count
            elif i==')':
                count-=1
            else:
                continue

        return max
