class Solution(object):
    def commonFactors(self, a, b):
        """
        :type a: int
        :type b: int
        :rtype: int
        """
        count=0
        c=max(a,b)
        for i in range(1,c):
            if(a%i==0 and b%i==0):
                  count+=1
            
        return count
                    
            
        
